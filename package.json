{
  "name": "unimported",
  "version": "1.9.0",
  "description": "Scans your nodejs project folder and shows obsolete files and modules",
  "main": "./dist/index.js",
  "bin": {
    "unimported": "bin/unimported.js"
  },
  "source": "./src/index.ts",
  "license": "MIT",
  "author": "Stephan Meijer <stephan@meijer.ws>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smeijer/unimported.git"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' --quiet --fix",
    "prepare": "npm run build",
    "run": "npm run build && node ./dist/index.js",
    "test": "jest",
    "ci:lint": "eslint -c ./.eslintrc.js",
    "ci:tsc": "tsc --noEmit --project ./tsconfig.json",
    "bump:patch": "npm version patch -m 'release: cut the %s release'",
    "bump:minor": "npm version minor -m 'release: cut the %s release'",
    "bump:major": "npm version major -m 'release: cut the %s release'",
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist",
    "bin"
  ],
  "keywords": [
    "unrequired",
    "redundant",
    "obsolete",
    "unimported",
    "unreferenced"
  ],
  "dependencies": {
    "@typescript-eslint/parser": "^4.15.2",
    "@typescript-eslint/typescript-estree": "^4.15.2",
    "chalk": "^4.1.0",
    "eslint": "^7.20.0",
    "flow-remove-types": "^2.145.0",
    "glob": "^7.1.6",
    "ora": "^5.3.0",
    "resolve": "^1.20.0",
    "term-size": "^2.2.1",
    "typescript": "^4.2.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/jest-in-case": "^1.0.3",
    "@types/node": "^14.14.31",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "console-testing-library": "^0.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
